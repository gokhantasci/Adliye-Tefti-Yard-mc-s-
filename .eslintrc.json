{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "script"
  },
  "globals": {
    "TeftisUtils": "readonly",
    "XLSX": "readonly",
    "JSZip": "readonly",
    "toast": "readonly",
    "showModal": "readonly",
    "hideModal": "readonly",
    "jQuery": "readonly",
    "$": "readonly"
  },
  "rules": {
    "no-console": "error",
    "no-unused-vars": "warn",
    "semi": ["error", "always"],
    "no-empty": "warn",
    "no-prototype-builtins": "warn"
  }
}
